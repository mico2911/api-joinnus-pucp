<header class="header-store">
    <a class="logo-joinnus" href="/tienda">
        <%- include('../svgs/logoJoinnus.ejs') %>
    </a>

    <% if (!autenticado) { %>
        <div class="options-auth">
            <a href="/ingresar" class="header-store-btn login-btn" type="button">Iniciar sesi√≥n</a>
            <a href="/registrarse" class="header-store-btn register-btn" type="button">Registrarse</a>
        </div>
    <% } else { %>
        <div>
            <a class="options-account" href="/<% if (usuario.isAdmin) { %>backoffice/listado-eventos<% } else { %>perfil<% } %>">
                <h5 class="m-0"> <%= usuario.nombre %> </h5>
                <img class="img-profile" src="https://graph.facebook.com/1524251321051930/picture?width=150&height=150" alt="userimg">
            </a>
        </div>
    <% } %> 
</header>